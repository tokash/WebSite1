<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="Data" type="System.Configuration.NameValueSectionHandler"/>
    <section name="SearchPatterns" type="System.Configuration.NameValueSectionHandler"/>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>
    <add key="FBPageList" value="FBPageList.txt" />
    <add key="ResultsLocation" value="C:\FBInformation\" />
  </appSettings>
  
  <Data>
    <add key="AccessTokenFileName" value="FBAccessToken.at" />
  </Data>

  <SearchPatterns>
    <add key="SearchPattern1" value=" [\u0591-\u05F4]\'[^(\u0591-\u05F4)]" />
    <add key="SearchPattern2" value="[\u0591-\u05F4][\u0591-\u05F4]\&quot;[\u0591-\u05F4] [\u0591-\u05F4]\'" />
    
  </SearchPatterns>
</configuration>